generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("POSTGRES_PRISMA_URL")
  directUrl = env("POSTGRES_URL_NON_POOLING")
}

model User {
  id String @id @default(cuid())
  name String
  email String
  // password String
  ciel_url String?
  aegis_url String?
  patio_url String?
}

model Ciel {
  id       String    @id @default(cuid())
}
